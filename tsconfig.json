{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "outDir": "./dist/",
    "lib": ["dom", "es2017"],
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es2015",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "strict": true
  }
}